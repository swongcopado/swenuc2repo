<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>It will list down the ProductCode for all the products whose basket records needs to be regenerated.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Is_Processed__c</fullName>
        <defaultValue>false</defaultValue>
        <description>If &quot;IsProcessed&quot; is checked/true, it means the cache response record has been regenerated for the given Product.</description>
        <externalId>false</externalId>
        <label>Is Processed</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Product_Code__c</fullName>
        <description>It will store the Product code value for which we need to regenerate the cache.</description>
        <externalId>false</externalId>
        <label>Product Code</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>sfiEnergyCAMModifiedProductsCode</label>
    <visibility>Public</visibility>
</CustomObject>
